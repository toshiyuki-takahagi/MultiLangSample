<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MultiLangSample.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MultiLangSample"
    xmlns:properties="clr-namespace:MultiLang.Resources.Properties;assembly=MultiLang.Resources"
    Shell.FlyoutBehavior="Disabled"
    Title="MultiLangSample">

    <ShellContent
        Title="{x:Static properties:Resources.MainPage_Title}"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
</Shell>
